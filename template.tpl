    const mockData = {
      feature: 'oneToOneIntegration'
    };

    runCode(mockData);
    assertApi('injectScript').wasCalled();
- name: Allow string as customer data
  code: |-
    const mockData = {
      feature: 'oneToOneIntegration',
      customerData: '{"name": "John Doe", "customerId": "1234"}'
